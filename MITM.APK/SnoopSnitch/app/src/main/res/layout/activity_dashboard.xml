<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="10"
    tools:context=".DashboardActivity" >

    <LinearLayout
        android:id="@+id/patchanalysis_summary"
        android:layout_width="match_parent"
        android:layout_height="14dp"
        android:layout_weight="1.5"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3.5"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="2"
                android:orientation="horizontal"
                android:weightSum="6">

                <ImageView
                    android:id="@+id/imgDashboardPatchanalysis"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:adjustViewBounds="false"
                    android:forceHasOverlappingRendering="false"
                    android:scaleType="fitStart"
                    android:src="@drawable/ic_patchanalysis" />

                <TextView
                    android:id="@+id/android_patch_analysis"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:text="@string/patchanalysis_label"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold"
                    android:visibility="visible" />

            </LinearLayout>

            <de.srlabs.patchanalysis_module.views.PatchanalysisSumResultChart
                android:id="@+id/sumResultChart"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_marginBottom="5dip"
                android:layout_weight="2"
                android:visibility="visible"
                custom:drawborder="true"
                custom:shownumbers="true"
                custom:small="false" />
        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/common_sectionSeparator" />

    <View
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:background="@color/common_applicationSeparator" />

    <LinearLayout
        android:id="@+id/network_security"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="4.5"
        android:background="@color/common_sectionBackground"
        android:orientation="horizontal"
        android:weightSum="10">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical"
            android:weightSum="17">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:orientation="horizontal"
                android:weightSum="2">

                <ImageView
                    android:id="@+id/ImageView04"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:scaleType="fitStart"
                    android:src="@drawable/ic_content_network" />

                <TextView
                    android:id="@+id/TextView08"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:gravity="left|center_vertical"
                    android:paddingTop="4dp"
                    android:text="@string/dashboard_network_security_title"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold" />

            </LinearLayout>

            <View
                android:id="@+id/View29"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/common_applicationSeparator" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center_vertical|center_horizontal"
                android:layout_weight="2"
                android:fontFamily="sans-serif-condensed"
                android:gravity="left|center_vertical"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="@string/dashboard_network_security_provider_scrollview_header"
                android:textColor="@color/common_text"
                android:textSize="@dimen/text_medium" />

            <View
                android:id="@+id/View30"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/common_applicationSeparator" />

            <ListView
                android:id="@+id/lstDashboardProviderList"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="10"
                android:background="@color/common_applicationBackground"
                android:divider="@null"
                android:fadeScrollbars="false"
                android:scrollbarFadeDuration="0"
                android:scrollbarStyle="insideOverlay"
                android:scrollbars="vertical"
                android:verticalScrollbarPosition="left">

            </ListView>

            <View
                android:id="@+id/View31"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/common_applicationSeparator" />

        </LinearLayout>

        <View
            android:id="@+id/View28"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/common_applicationSeparator" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:orientation="vertical"
            android:weightSum="17">

            <LinearLayout
                android:id="@+id/network_scan_info_and_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:weightSum="2">

                <TextView
                    android:id="@+id/txtDashboardNetworkTest"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:layout_weight="2"
                    android:gravity="center_vertical|center_horizontal"
                    android:textSize="@dimen/text_medium" />

                <Button
                    android:id="@+id/btnDashboardTestNetwork"
                    style="@style/Widget.AppCompat.Button.Small"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical|center_horizontal"
                    android:onClick="toggleNetworkTest"
                    android:text="@string/common_button_networktest_start"
                    android:textColor="@drawable/custom_upload_button_color"
                    android:textSize="12sp" />

            </LinearLayout>

            <View
                android:id="@+id/View34"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/chart_headers"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2">

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/dashboard_network_security_interception_title"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/View35"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/common_applicationSeparator" />

                <TextView
                    android:id="@+id/TextView07"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/dashboard_network_security_impersonation_title"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold" />

            </LinearLayout>

            <View
                android:id="@+id/View33"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/common_applicationSeparator" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible"
                    android:weightSum="2">

                    <de.srlabs.snoopsnitch.views.DashboardProviderChart
                        android:id="@+id/pvcDashboardInterception"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:onClick="openLocalMapView"
                        android:visibility="visible"
                        custom:type="interception" />

                    <View
                        android:id="@+id/View39"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/common_applicationSeparator" />

                    <de.srlabs.snoopsnitch.views.DashboardProviderChart
                        android:id="@+id/pvcDashboardImpersonation"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:onClick="openLocalMapView"
                        android:visibility="visible"
                        custom:type="impersonation" />

                </LinearLayout>

                <View
                    android:id="@+id/View40"
                    android:layout_width="10dp"
                    android:layout_height="33dp"
                    android:layout_gravity="top|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@color/common_sectionBackground" />

                <View
                    android:id="@+id/View41"
                    android:layout_width="10dp"
                    android:layout_height="33dp"
                    android:layout_gravity="bottom|center_horizontal"
                    android:layout_marginBottom="6dp"
                    android:background="@color/common_sectionBackground" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center_vertical|center_horizontal"
                    android:paddingBottom="2dp"
                    android:paddingTop="2dp"
                    android:text="@string/dashboard_network_security_protectionLevel_high"
                    android:textSize="@dimen/text_medium" />

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center_horizontal"
                    android:layout_marginBottom="8dp"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center_vertical|center_horizontal"
                    android:paddingBottom="2dp"
                    android:paddingTop="2dp"
                    android:text="@string/dashboard_network_security_protectionLevel_low"
                    android:textSize="@dimen/text_medium" />

            </FrameLayout>

            <View
                android:id="@+id/View32"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/chart_descriptions"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:weightSum="4">

                <TextView
                    android:id="@+id/txtDashboardInterception3g"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/common_3g_title"
                    android:textColor="@color/common_text"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/View37"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="3dp"
                    android:layout_marginTop="3dp"
                    android:background="@color/common_applicationSeparator" />

                <TextView
                    android:id="@+id/txtDashboardInterception2g"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/common_2g_title"
                    android:textColor="@color/common_text"
                    android:textSize="10sp" />

                <View
                    android:id="@+id/View38"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/common_applicationSeparator" />

                <!--
                <TextView
                    android:id="@+id/txtDashboardImpersonation3g"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/common_3g_title"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold"
                    android:textColor="@color/common_text"
                    android:fontFamily="sans-serif-condensed" />

                <View
                    android:id="@+id/View36"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="3dp"
                    android:layout_marginTop="3dp"
                    android:background="@color/common_applicationSeparator" />

                <TextView
                    android:id="@+id/txtDashboardImpersonation2g"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/common_2g_title"
                    android:textSize="@dimen/text_medium"
                    android:textColor="@color/common_text"
                    android:fontFamily="sans-serif-condensed" />
              	-->

                <TextView
                    android:id="@+id/txtDashboardImpersonation2g"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/common_2g_title"
                    android:textColor="@color/common_text"
                    android:textSize="10sp" />

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/common_sectionSeparator" />

    <View
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:background="@color/common_applicationSeparator" />

    <LinearLayout
        android:id="@+id/dashboardChartSection"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="4"
        android:background="@color/common_sectionBackground"
        android:orientation="vertical"
        android:weightSum="8">

        <LinearLayout
            android:id="@+id/SilentSMSCharts"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:onClick="openDetailView"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="3"
                android:orientation="horizontal"
                android:weightSum="6">

                <ImageView
                    android:id="@+id/imgDashboardSilentSms"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:scaleType="fitStart"
                    android:src="@drawable/ic_content_sms_event" />

                <TextView
                    android:id="@+id/TextView02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:text="@string/dashboard_sms_description"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold" />

            </LinearLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/dashboardSmsChartSectionMonth"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardSilentSmsMonthCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/SilentSMSChartMonth"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="month"
                    custom:typeOfThreatDashboard="silentSMS" />
            </LinearLayout>

            <View
                android:id="@+id/View06"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/dashboardSmsChartSectionWeek"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardSilentSmsWeekCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/SilentSMSChartWeek"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="week"
                    custom:typeOfThreatDashboard="silentSMS" />
            </LinearLayout>

            <View
                android:id="@+id/View07"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/dashboardSmsChartSectionDay"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardSilentSmsDayCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/SilentSMSChartDay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="day"
                    custom:typeOfThreatDashboard="silentSMS" />
            </LinearLayout>

            <View
                android:id="@+id/View24"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:id="@+id/dashboardSmsChartSectionHour"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardSilentSmsHourCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:tag="threatCount"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/SilentSMSChartHour"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="hour"
                    custom:typeOfThreatDashboard="silentSMS" />

            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/View23"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/common_applicationSeparator" />

        <LinearLayout
            android:id="@+id/IMSICatcherCharts"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:onClick="openDetailView"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="3"
                android:orientation="horizontal"
                android:weightSum="6">

                <ImageView
                    android:id="@+id/imgDashboardImsiCatcher"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:scaleType="fitStart"
                    android:src="@drawable/ic_content_imsi_event" />

                <TextView
                    android:id="@+id/TextView06"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:text="@string/common_imsi_catcher"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/text_large"
                    android:textStyle="bold" />

            </LinearLayout>

            <View
                android:id="@+id/View05"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardImsiCatcherMonthCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/IMSICatcherChartMonth"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="month"
                    custom:typeOfThreatDashboard="imsiCatcher" />
            </LinearLayout>

            <View
                android:id="@+id/View08"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardImsiCatcherWeekCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/IMSICatcherChartWeek"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="week"
                    custom:typeOfThreatDashboard="imsiCatcher" />
            </LinearLayout>

            <View
                android:id="@+id/View09"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/common_applicationSeparator" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtDashboardImsiCatcherDayCount"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="1"
                    android:textColor="@color/common_chartGreen"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold" />

                <de.srlabs.snoopsnitch.views.DashboardThreatChart
                    android:id="@+id/IMSICatcherChartDay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    custom:timePeriodDashboard="day"
                    custom:typeOfThreatDashboard="imsiCatcher" />
            </LinearLayout>

            <View
                android:id="@+id/View18"
                android:layout_width="1dp"
                android:layout_height="match_parent"
